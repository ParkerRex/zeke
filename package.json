{
  "name": "zeke",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "ci": "pnpm run validate && pnpm run build",
    "clean": "rm -rf .next node_modules",
    "db:migrate": "pnpm supabase migration up --local && pnpm run types:generate",
    "db:reset": "pnpm supabase db reset --local",
    "dev": "bash scripts/dev-start.sh && pnpm run dev:full",
    "dev:full": "concurrently --names \"NEXT,WORKER\" --prefix-colors \"green,yellow\" \"pnpm run dev:next\" \"PORT=8082 bash worker/scripts/deploy-local-worker.sh\"",
    "worker:up": "PORT=8082 bash worker/scripts/deploy-local-worker.sh",
    "worker:rebuild": "bash scripts/worker-rebuild-run-8082.sh",
    "worker:logs": "docker logs -f zeke-worker-local-8082",
    "worker:stop": "bash scripts/worker-stop.sh",
    "db:clean:youtube": "bash scripts/db-clean-youtube-bad.sh",
    "dev:next": "next dev --turbopack --port 3000",
    "dev:setup": "bash scripts/dev-start.sh",
    "bootstrap": "bash scripts/bootstrap.sh",
    "email:dev": "email dev --dir ./src/emails --port 3001",
    "lint": "next lint && cd worker && pnpm run lint",
    "fix:biome": "biome lint --apply .",
    "lint:main": "next lint",
    "lint:worker": "cd worker && pnpm run lint",
    "migration:new": "pnpm supabase migration new",
    "migration:squash": "pnpm supabase migration squash --linked",
    "migration:up": "pnpm supabase migration up --linked --debug && pnpm run types:generate:remote",
    "start": "next start",
    "stop": "bash scripts/dev-stop.sh",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks --project-name=default",
    "ingest:rss:now": "curl -fsS -X POST http://localhost:8080/debug/ingest-now || true",
    "ingest:youtube:now": "curl -fsS -X POST http://localhost:8080/debug/ingest-youtube || true",
    "status": "curl -fsS http://localhost:8080/debug/status || true",
    "test": "pnpm run typecheck && pnpm run test:db && pnpm run test:worker",
    "typecheck": "tsc --noEmit && cd worker && tsc --noEmit",
    "types:generate": "pnpm supabase gen types typescript --local --schema public > src/lib/supabase/types.ts",
    "types:generate:remote": "env-cmd -f ./.env.production pnpm supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/supabase/types.ts",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run lint:biome",
    "worker:deploy": "cd worker && pnpm run deploy:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
