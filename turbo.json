{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "ui": "stream",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        "dist/**",
        "build/**",
        "lib/**"
      ],
      "passThroughEnv": [
        "SUPABASE_SERVICE_KEY",
        "SUPABASE_API_KEY",
        "RESEND_API_KEY",
        "RESEND_AUDIENCE_ID",
        "UPSTASH_REDIS_REST_URL",
        "UPSTASH_REDIS_REST_TOKEN",
        "API_ROUTE_SECRET",
        "ENGINE_API_KEY",
        "GITHUB_TOKEN",
        "OPENAI_API_KEY",
        "MISTRAL_API_KEY",
        "OPENPANEL_SECRET_KEY",
        "NEXT_PUBLIC_URL",
        "VERCEL_TARGET_ENV",
        "SENTRY_AUTH_TOKEN",
        "SENTRY_ORG",
        "SENTRY_PROJECT",
        "NEXT_PUBLIC_SENTRY_DSN",
        "DATABASE_URL",
        "WEBHOOK_SECRET_KEY",
        "YOUTUBE_API_KEY",
        "YOUTUBE_QUOTA_LIMIT",
        "YOUTUBE_QUOTA_RESET_HOUR",
        "YOUTUBE_RATE_LIMIT_BUFFER",
        "YOUTUBE_OAUTH_CLIENT_ID",
        "YOUTUBE_OAUTH_CLIENT_SECRET",
        "YOUTUBE_OAUTH_REFRESH_TOKEN",
        "JOBS_CRON_TZ",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_OPENPANEL_CLIENT_ID",
        "OPENAI_CHAT_MODEL",
        "OPENAI_EMBEDDING_MODEL",
        "RAILWAY_ENVIRONMENT",
        "PORT",
        "NODE_ENV"
      ]
    },
    "start": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "persistent": true,
      "cache": false,
      "passThroughEnv": [
        "DATABASE_PRIMARY_POOLER_URL",
        "DATABASE_SESSION_POOLER_URL",
        "DATABASE_URL",
        "TRIGGER_PROJECT_ID",
        "TRIGGER_SECRET_KEY",
        "OPENAI_API_KEY",
        "SUPABASE_SERVICE_KEY",
        "SUPABASE_API_KEY",
        "RESEND_API_KEY",
        "RESEND_AUDIENCE_ID",
        "UPSTASH_REDIS_REST_URL",
        "UPSTASH_REDIS_REST_TOKEN",
        "API_ROUTE_SECRET",
        "ENGINE_API_KEY",
        "GITHUB_TOKEN",
        "MISTRAL_API_KEY",
        "OPENPANEL_SECRET_KEY",
        "NEXT_PUBLIC_URL",
        "WEBHOOK_SECRET_KEY",
        "YOUTUBE_API_KEY",
        "YOUTUBE_QUOTA_LIMIT",
        "YOUTUBE_QUOTA_RESET_HOUR",
        "YOUTUBE_RATE_LIMIT_BUFFER",
        "YOUTUBE_OAUTH_CLIENT_ID",
        "YOUTUBE_OAUTH_CLIENT_SECRET",
        "YOUTUBE_OAUTH_REFRESH_TOKEN",
        "JOBS_CRON_TZ",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_OPENPANEL_CLIENT_ID",
        "OPENAI_CHAT_MODEL",
        "OPENAI_EMBEDDING_MODEL",
        "RAILWAY_ENVIRONMENT",
        "PORT",
        "NODE_ENV",
        "TZ"
      ]
    },
    "jobs": {
      "persistent": true,
      "cache": false
    },
    "format": {},
    "lint": {
      "dependsOn": ["^topo"]
    },
    "typecheck": {
      "dependsOn": ["^topo"],
      "outputs": []
    }
  }
}
