# Supabase Postgres connection string (Session Pooler) for Cloud Run
# Use ?sslmode=no-verify with Cloud Run unless you configure CA trust.
DATABASE_URL=postgresql://worker.<project-ref>:<PASSWORD>@<region>.pooler.supabase.com:5432/postgres?sslmode=no-verify

# pg-boss schema and timezone
BOSS_SCHEMA=pgboss
BOSS_CRON_TZ=UTC

# Cloud Run deploy settings
PROJECT_ID=your-gcp-project-id
REGION=us-central1
SERVICE=zeke-worker
BOSS_MIGRATE=false

# OpenAI API Configuration
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

